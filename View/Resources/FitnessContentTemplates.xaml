<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:userControls="clr-namespace:Fitness.View.UserControls"
                    xmlns:types="clr-namespace:Fitness.Common.FitnessTabContents;assembly=Fitness.Common">
    <DataTemplate x:Key="HomeTemplate"
                  DataType="{x:Type types:IHomeContent}">
        <userControls:Home/>
    </DataTemplate>
    <DataTemplate x:Key="ClientOperationsTemplate"
                  DataType="{x:Type types:IClientOperationsContent}">
        <userControls:ClientOperations/>
    </DataTemplate>
    <DataTemplate x:Key="AddNewClientTemplate"
                  DataType="{x:Type types:INewClientContent}">
        <userControls:AddNewClient/>
    </DataTemplate>

</ResourceDictionary>